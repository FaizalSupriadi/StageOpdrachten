lumn":20}},{"source":"ts/pool.ts","name":null,"original":{"line":47,"column":0},"generated":{"line":112,"column":29}}],"sources":{"ts/pool.ts":"class Particle{\r\n    public lifetime: number = 3;\r\n    public is_used() { \r\n        return this.lifetime > 0; \r\n    }\r\n\r\n    public print(){\r\n        console.log(\"Particle\");\r\n    }\r\n}\r\n\r\nclass ParticlePool{\r\n    private poolSize: number = 50;\r\n    private particlePool: Particle[] = [];\r\n\r\n    public create(amount:number=1){\r\n        for(let i = 0; i<amount; i++ ){\r\n            if(this.particlePool.length < this.poolSize){\r\n                this.particlePool.push(new Particle());\r\n            }else{\r\n                console.log(\"Pool is full\");\r\n            }\r\n        }\r\n    }\r\n    public update(amount:number=1){\r\n        for(let i = 0; i<amount; i++ ){\r\n            for (const particle of this.particlePool) {\r\n                if(!particle.is_used()){\r\n                    this.delete(this.particlePool[0]);\r\n                }else{\r\n                    particle.print();\r\n                    particle.lifetime -= 1;\r\n                }\r\n            }\r\n        }\r\n    }\r\n\r\n    public delete(particle:Particle){\r\n        const particleIndex = this.particlePool.indexOf(particle);\r\n        if (particleIndex != -1) {\r\n            this.particlePool.splice(particleIndex,1);\r\n        }\r\n        this.update();\r\n    }\r\n}\r\n\r\nexport function test_pool(){\r\n    console.log(\"\");\r\n    let particlePool = new ParticlePool();\r\n    particlePool.create();\r\n    particlePool.update();\r\n}"},"lineCount":null}},"error":null,"hash":"0a4a6892a640991028fc06d8bb0b3a38","cacheData":{"env":{}}}